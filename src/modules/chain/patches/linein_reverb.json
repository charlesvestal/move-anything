{
    "name": "Line In + Reverb",
    "version": 1,
    "chain": {
        "input": "both",
        "synth": {
            "module": "linein",
            "config": {}
        },
        "audio_fx": [
            {
                "type": "freeverb",
                "params": {
                    "room_size": 0.7,
                    "damping": 0.5,
                    "wet": 0.35,
                    "dry": 0.65
                }
            }
        ],
        "knob_mappings": [
            {"cc": 71, "target": "fx1", "param": "room_size"},
            {"cc": 72, "target": "fx1", "param": "damping"},
            {"cc": 73, "target": "fx1", "param": "wet"},
            {"cc": 74, "target": "fx1", "param": "dry"}
        ]
    }
}
